{
  "name": "my-conciliate-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "pnpm run build-wrappers",
    "dev": "next dev | grep --color=always -v 'nextjs_original-stack-frames'",
    "prebuild": "pnpm run build-wrappers",
    "build": "next build",
    "build:plain": "next build",
    "build-wrappers": "pnpm --filter lilypad-wrapper build && pnpm --filter lit-wrapper build && pnpm --filter web-storage-wrapper build",
    "start": "next start",
    "lint": "next lint",
    "lint:biome": "biome lint .",
    "check": "biome check --apply .",
    "format": "biome format --write .",
    "prepages:build": "pnpm run build-wrappers",
    "pages:build": "pnpm dlx @cloudflare/next-on-pages@1",
    "pages:dev": "pnpm dlx wrangler pages dev --compatibility-flags nodejs_compat --compatibility-date 2025-04-08"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@stytch/nextjs": "^21.4.2",
    "@stytch/vanilla-js": "^5.21.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "ethers": "5",
    "events-browserify": "^0.0.1",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.2.0",
    "input-otp": "^1.4.2",
    "lilypad-wrapper": "workspace:*",
    "lit-wrapper": "workspace:*",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-themes": "^0.4.6",
    "openai": "^4.91.1",
    "pinata-web3": "^0.5.4",
    "pino-pretty": "^13.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.3",
    "stytch": "^12.14.0",
    "tailwind-merge": "^3.1.0",
    "undici": "^7.8.0",
    "utf-8-validate": "^6.0.5",
    "uuid": "^11.1.0",
    "viem": "^2.25.0",
    "web-storage-wrapper": "workspace:*",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.25.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "rollup": "^4.39.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "util": "^0.12.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "protobufjs",
      "sharp",
      "utf-8-validate"
    ]
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@walletconnect/types": "2.9.2",
    "@noble/ed25519": "2.1.0"
  },
  "packageManager": "pnpm@10.7.1"
}
