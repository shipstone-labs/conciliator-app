    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lilypad-wrapper": "workspace:*",
    "lit-wrapper": "workspace:*",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "openai": "^4.91.1",
    "pinata-web3": "^0.5.4",
    "pino-pretty": "^13.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
