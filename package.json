    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.25.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.1.7",
    "events": "^3.3.0",
    "postcss": "^8.5.3",
    "process": "^0.11.10",
