{
  "name": "my-conciliate-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "pnpm run build-wrappers",
    "dev": "next dev | grep --color=always -v 'nextjs_original-stack-frames'",
    "prebuild": "pnpm run build-wrappers",
